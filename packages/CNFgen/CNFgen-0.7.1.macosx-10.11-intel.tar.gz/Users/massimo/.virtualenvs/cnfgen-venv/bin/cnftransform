#!/Users/massimo/.virtualenvs/cnfgen-venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'CNFgen==0.7.1','console_scripts','cnftransform'
__requires__ = 'CNFgen==0.7.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('CNFgen==0.7.1', 'console_scripts', 'cnftransform')()
    )
