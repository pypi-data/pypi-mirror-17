#!/home/travis/virtualenv/python3.5.0/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'csirtgsdk==0.0.0a6','console_scripts','csirtg'
__requires__ = 'csirtgsdk==0.0.0a6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('csirtgsdk==0.0.0a6', 'console_scripts', 'csirtg')()
    )
