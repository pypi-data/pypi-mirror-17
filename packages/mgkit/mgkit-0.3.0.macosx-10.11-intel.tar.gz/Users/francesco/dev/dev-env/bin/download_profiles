#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'mgkit==0.3.0','console_scripts','download_profiles'
__requires__ = 'mgkit==0.3.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('mgkit==0.3.0', 'console_scripts', 'download_profiles')()
    )
