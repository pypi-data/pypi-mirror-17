[mapping]
assembly = GRCh38
alt_aln_method = splign
normalize = True
inferred_p_is_uncertain = True
replace_reference = True

[normalizer]
window_size = 20
cross_boundaries = False
shuffle_direction = 3

[uta]
prd_version = uta_20150906
stg_version = uta_20160908
dev_version = uta_1_1
local_url_base = postgresql://anonymous:anonymous@localhost/uta
public_url_base = postgresql://anonymous:anonymous@uta.biocommons.org/uta


local =      postgresql://anonymous:anonymous@localhost/uta/${prd_version}
public =     postgresql://anonymous:anonymous@uta.biocommons.org/uta/${prd_version}
local_dev =  postgresql://anonymous:anonymous@localhost/uta_dev/${dev_version}
public_dev = postgresql://anonymous:anonymous@uta.biocommons.org/uta_dev/${dev_version}
