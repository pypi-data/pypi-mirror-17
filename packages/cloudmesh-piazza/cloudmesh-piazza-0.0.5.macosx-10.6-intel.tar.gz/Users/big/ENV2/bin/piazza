#!/Users/big/ENV2/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'cloudmesh-piazza==0.0.5','console_scripts','piazza'
__requires__ = 'cloudmesh-piazza==0.0.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cloudmesh-piazza==0.0.5', 'console_scripts', 'piazza')()
    )
