#!/home/jacopo/dev/efesto/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'efesto==0.7','console_scripts','efesto-quickstart'
__requires__ = 'efesto==0.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('efesto==0.7', 'console_scripts', 'efesto-quickstart')()
    )
