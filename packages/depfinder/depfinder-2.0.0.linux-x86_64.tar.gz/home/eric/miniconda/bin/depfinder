#!/home/eric/miniconda/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'depfinder==2.0.0','console_scripts','depfinder'
__requires__ = 'depfinder==2.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('depfinder==2.0.0', 'console_scripts', 'depfinder')()
    )
