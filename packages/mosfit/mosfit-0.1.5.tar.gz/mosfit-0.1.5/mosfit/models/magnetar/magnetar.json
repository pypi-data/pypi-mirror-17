{
    "transient":{
        "kind":"data",
        "keys":{
            "photometry":{
                "time":["required", "numeric"],
                "magnitude":["required", "numeric"],
                "e_magnitude":["required", "numeric"],
                "band":"required",
                "instrument":"optional",
                "system":"optional",
                "upperlimit":"exclude",
                "includeshost":"exclude",
                "host":"exclude",
                "kcorrected":"exclude",
                "scorrected":"exclude"
            },
            "ebv":{
                "value":"required"
            },
            "lumdist":{
                "value":"required"
            },
            "redshift":{
                "value":"required"
            }
        }
    },
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "lumdist":{
        "kind":"parameter",
        "value":463.4,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"d_{L}"
    },
    "redshift":{
        "kind":"parameter",
        "value":0.1,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"z"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e18,
        "class":"parameter",
        "latex":"n_{\\rm H,host}"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}"
    },
    "Pspin":{
        "kind":"parameter",
        "value":2.0,
        "class":"parameter",
        "latex":"P_{\\rm spin}\\,{\\rm (ms)}"
    },
    "Bfield":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"B"
    },
    "Mns":{
        "kind":"parameter",
        "value":2.0,
        "class":"parameter",
        "latex":"M_{\\rm NS}\\,(M_\\odot)"
    },
    "thetaPB":{
        "kind":"parameter",
        "value":0.785398163,
        "class":"parameter",
        "latex":"\\theta_{\\rm PB}"
    },
    "kappa":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "vejecta":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "variance":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"\\sigma"
    },
    "densetimes":{
        "kind":"array",
        "inputs":[
            "transient",
            "texplosion"
        ]
    },
    "magnetar":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta",
            "Pspin",
            "Bfield",
            "Mns",
            "thetaPB",
            "texplosion"
        ]
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "magnetar",
            "kappa",
            "kappagamma",
            "mejecta",
            "texplosion",
            "vejecta"
        ]
    },
    "densecore":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "diffusion"
        ]
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "densecore",
            "temperaturephot",
            "radiusphot"
        ]
    },
    "extinction":{
        "kind":"sed",
        "inputs":[
            "blackbody",
            "nhhost",
            "ebv"
        ],
        "requests":[
            ["samplewavelengths", "filters"],
            [],
            []
        ]
    },
    "filters":{
        "kind":"observable",
        "inputs":[
            "extinction",
            "lumdist"
        ],
        "requests":[
            ["band_wave_ranges", "filters"],
            []
        ]
    },
    "likelihood":{
        "kind":"objective",
        "inputs":[
            "transient",
            "variance",
            "filters"
        ]
    },
    "fitlc":{
        "kind":"output",
        "inputs":[
            "likelihood",
            "transient",
            "filters"
        ],
        "class":"lightcurve"
    }
}
