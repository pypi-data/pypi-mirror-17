<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">

  <browser:resourceDirectory
      name="collective.feedaggregator"
      directory="static"
      />

  <browser:page
      name="listing_view"
      for="collective.feedaggregator.interfaces.IFeedAggregator"
      permission="zope2.View"
      class="collective.feedaggregator.browser.ListingView"
      />

</configure>
